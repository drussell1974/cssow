  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "clean-dir": "^1.0.3",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "scripts": {
    "clean": "cleandir ../../src/teacher_web/web/static/default/react_apps/build/notification-app",
    "build:dev": "env-cmd -f ../../dotenv/.env.development webpack --mode development && cp -R ./build ../../src/teacher_web/web/static/default/react_apps/",
    "build": "yarn clean && env-cmd -f ../../dotenv/.env.production webpack --mode production && tar -czf ../../build/react_apps/notification-app.build.tar.gz build",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-bridge.0",
    "@babel/plugin-transform-runtime": "^7.7.5",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "copy": "^0.3.2",
    "copy-webpack-plugin": "^5.1.1",
    "cors": "^2.8.5",
    "css-loader": "^3.3.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "marked": "^1.1.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "whatwg-fetch": "^3.6.2"
  },
  "jest": {
    "rootDir": "app/",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/",
      "/services/",
      "<rootDir>/services/",
      "/pages/",
      "<rootDir>/pages/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
